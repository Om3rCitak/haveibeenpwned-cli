{
  "alias": "haveibeenpwned.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Json"
  ],
  "directories": [
    "src"
  ],
  "extract": true,
  "files": [
    "README.md"
  ],
